package M1;

import java.util.*;

public class fibonacci {



	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int range,i;

		System.out.print("Enter the range upto:\t");

		range = sc.nextInt();

		System.out.println("Fibonacci series upto "+range);

		int a = 0,b = 1;

		while(a<range)

		{

			System.out.print("\t"+a);

			int c = a+b;

			a = b;

			b = c;

		}

		



	}



}

